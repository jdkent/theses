@Article{	  botvinick2015,
  abstract	= {Research on cognitive control and executive function has
		  long recognized the relevance of motivational factors.
		  Recently, however, the topic has come increasingly to
		  center stage, with a surge of new studies examining the
		  in-terface of motivation and cognitive control. In the
		  present article we survey research situated at this
		  interface, considering work from cognitive and social
		  psychology and behavioral economics, but with a particular
		  focus on neuro-science research. We organize existing
		  findings into three core areas, consid-ering them in the
		  light of currently vying theoretical perspectives. Based on
		  the accumulated evidence, we advocate for a view of control
		  function that treats it as a domain of reward-based
		  decision making. More broadly, we argue that
		  neuroscientific evidence plays a critical role in
		  understanding the mechanisms by which motivation and
		  cognitive control interact. Opportu-nities for further
		  cross-fertilization between behavioral and neuroscientific
		  research are highlighted.},
  annote	= {cognitive control definition: set of superorfinate
		  functions that encode and maintain a representation of the
		  current task (i.e. contextually relevant stimulus-response
		  associations, action-outcome contingencies, and target
		  states or goals, marchaling to that task subordinate
		  functions including working, semantic, and episodic
		  memory,; perceptual attention; and action selection and
		  inhibition.},
  author	= {Botvinick, Matthew and Braver, Todd},
  doi		= {10.1146/annurev-psych-010814-015044},
  file		= {:media/Data/Downloads/936130a303493a67791df4b1cda87679b0ce.pdf:pdf},
  isbn		= {1545-2085 (Electronic) 0066-4308 (Linking)},
  issn		= {0066-4308},
  journal	= {Annual Review of Psychology},
  keywords	= {cognitive control,effort,motivation,prefrontal
		  cortex,reward},
  number	= {1},
  pages		= {83--113},
  pmid		= {25251491},
  title		= {{Motivation and Cognitive Control: From Behavior to Neural
		  Mechanism}},
  url		= {http://www.annualreviews.org/doi/10.1146/annurev-psych-010814-015044},
  volume	= {66},
  year		= {2015}
}

