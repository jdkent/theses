@Article{	  ungless2004,
  abstract	= {There is general agreement that midbrain dopamine neurons
		  play key roles in reward processing. What is more
		  controversial is the role they play in processing salient
		  stimuli that are not rewarding. This controversy has arisen
		  for three main reasons. First, salient sensory stimuli such
		  as tones and lights, which are assumed not to be rewarding,
		  increase dopamine neuron activity. Second, aversive stimuli
		  increase firing in a minority of putative dopamine neurons.
		  Third, dopamine release is increased following aversive
		  stimuli. Consequently, it has been suggested that these
		  midbrain dopamine neurons are activated by all salient
		  stimuli, rather than specifically by rewards. However,
		  reconsideration of these issues, in light of new findings,
		  suggests this controversy can be resolved in favour of
		  reward theories.},
  annote	= {Does dopamine encode salience?
		  
		  1) salient sensory stimuli that do not appear to be
		  rewarding evoke activations of dopamine neurons problems:
		  a) these activations could represent generalizations to
		  stimuli that are associated with reward b) novelty itself
		  can be rewarding c) salient sensory stimuli could have
		  rewarding properties, although not immediately obvious to
		  the experimenter. 2) aversive stimuli increae firing in a
		  minority of putative dopamine neurons a) response, no: if
		  you teach a rat that tone predicts reward ommision, then
		  there isn't an increase in firing, despite the stimulus
		  being attention grabbing 3) dopamine releasse is increased
		  following aversive stimuli response: are they really
		  dopaminergic? don't trust electrophysiological
		  characteristics},
  author	= {Ungless, Mark A.},
