  keywords	= {*Auditory Perception,*Cues,*Lateral Dominance,*Reaction
		  Time,Stimulus Presentation Methods},
  number	= {3},
  pages		= {300--304},
  publisher	= {American Psychological Association},
  title		= {{Auditory S-R compatibility: The effect of an irrelevant
		  cue on information processing.}},
