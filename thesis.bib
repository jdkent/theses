@Article{	  strange2014,
  abstract	= {Animal models and human functional imaging data implicate
		  the dopamine system in mediating enhanced encoding of novel
		  stimuli into human memory. A separate line of investigation
		  suggests an association between a functional polymorphism
		  in the promoter region for the human dopamine 4 receptor
		  gene (DRD4) and sensitivity to novelty. We demonstrate, in
		  two independent samples, that the -521C{\textgreater}T DRD4
		  promoter polymorphism determines the magnitude of human
		  memory enhancement for contextually novel, perceptual
		  oddball stimuli in an allele dose-dependent manner. The
		  genotype-dependent memory enhancement conferred by the C
		  allele is associated with increased neuronal responses
		  during successful encoding of perceptual oddballs in the
		  ventral striatum, an effect which is again allele
		  dose-dependent. Furthermore, with repeated presentations of
		  oddball stimuli, this memory advantage decreases, an effect
		  mirrored by adaptation of activation in the hippocampus and
		  substantia nigra/ventral tegmental area in C carriers only.
		  Thus, a dynamic modulation of human memory enhancement for
		  perceptually salient stimuli is associated with activation
		  of a dopaminergic-hippocampal system, which is critically
		  dependent on a functional polymorphism in the DRD4 promoter
		  region. {\textcopyright} 2013 Elsevier Inc.},
  author	= {Strange, B. A. and Gartmann, N. and Brenninkmeyer, J. and
		  Haaker, J. and Reif, A. and Kalisch, R. and B{\"{u}}chel,
		  C.},
  doi		= {10.1016/j.neuroimage.2013.09.065},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Strange et al. - 2014 - Dopamine
		  receptor 4 promoter polymorphism modulates memory and
		  neuronal responses to salience.pdf:pdf},
  isbn		= {1053-8119(Print)},
  issn		= {10959572},
  journal	= {NeuroImage},
  keywords	= {DRD4 promoter
		  polymorphism,Dopamine,FMRI,Hippocampus,Memory,Novelty,dopamine,fmri,oddball,task},
  mendeley-tags	= {dopamine,fmri,oddball,task},
  pages		= {922--931},
  pmid		= {24099848},
  publisher	= {Elsevier Inc.},
  title		= {{Dopamine receptor 4 promoter polymorphism modulates
		  memory and neuronal responses to salience}},
  url		= {http://dx.doi.org/10.1016/j.neuroimage.2013.09.065},
  volume	= {84},
  year		= {2014}
}

