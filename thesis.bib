@Article{	  norman1986,
  abstract	= {Much effort has been made to understand the role of
		  attention in perception; much less effort has been placed
		  on the role attention plays in the control of action. Our
		  goal in this chapter is to account for the role of
		  attention in action, both when performance is automatic and
		  when it is under deliberate conscious control. We propose a
		  theoretical framework structured around the notion of a set
		  of active schemas, organized according to the particular
		  action sequences of which they are a part, awaiting the
		  appropriate set of conditions so that they can become
		  selected to control action. The analysis is therefore
		  centered around actions, primarily external actions, but
		  the same principles apply to internal actions—actions
		  that involve only the cognitive processing mechanisms. One
		  major emphasis in the study of attentional processes is the
		  distinction between controlled and automatic processing of
		  perceptual inputs (e.g., Shiffrin {\&} Schneider, 1977).
		  Our work here can be seen as complementary to the
		  distinction between controlled and automatic processes: we
		  examine action rather than perception; we emphasize the
		  situations in which deliberate, conscious control of
		  activity is desired rather than those that are automatic.},
  annote	= {three definitions of automatic 1) the way certain tasks
		  can be executed without awareness of their performance
		  (example: walking) 2) initiated and performed without
		  deliberate attention or awareness (example: automatic
		  brushing away of an insect from one's arm) 3) orienting
		  response, in which attention is drawn "automatically" to
		  something, with no deliberate control over the direction of
		  attention.
		  
		  Actions that can occur between schemas: 1) selection 2)
		  competition 3) negotiation
		  
		  states of a schema: 1) dormant: resides in permanent memory
		  structure 2) activated: setup, brought to a state of
		  readiness and given an activation value 3) selected: when
		  the activation value is sufficiently high to exceed its own
		  threshold},
  author	= {Norman, Donald A. and Shallice, Tim},
  doi		= {10.1007/978-1-4757-0629-1_1},
  file		= {:media/Data/Downloads/ADA094713.pdf:pdf},
  isbn		= {978-1-4757-0631-4},
  issn		= {0962-8436},
  journal	= {Consciousness {\&} Self-Regulation},
  keywords	= {cognitive control,theory},
  mendeley-tags	= {cognitive control,theory},
  pages		= {1--18},
  pmid		= {8941959},
  title		= {{Attention to Action}},
  url		= {http://link.springer.com/chapter/10.1007/978-1-4757-0629-1{\_}1{\%}5Cnhttp://dx.doi.org/10.1007/978-1-4757-0629-1{\_}1{\%}5Cnhttp://scholar.google.com/scholar?hl=en{\&}btnG=Search{\&}q=intitle:Attention+to+Action:+Willed+and+Automatic+Control+of+Behaviour{\#}1{\%}5Cnhttp://schol},
  volume	= {4},
