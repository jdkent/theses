@Article{	  suo2016,
  abstract	= {Physical and cognitive exercise may prevent or delay
		  dementia in later life but the neural mechanisms underlying
		  these therapeutic benefits are largely unknown. We examined
		  structural and functional magnetic resonance imaging (MRI)
		  brain changes after 6 months of progressive resistance
		  training (PRT), computerized cognitive training (CCT) or
		  combined intervention. A total of 100 older individuals (68
		  females, average age=70.1, s.d.+/-6.7, 55-87 years) with
		  dementia prodrome mild cognitive impairment were recruited
		  in the SMART (Study of Mental Activity and Resistance
		  Training) Trial. Participants were randomly assigned into
		  four intervention groups: PRT+CCT, PRT+SHAM CCT, CCT+SHAM
		  PRT and double SHAM. Multimodal MRI was conducted at
		  baseline and at 6 months of follow-up (immediately after
		  training) to measure structural and spontaneous functional
		  changes in the brain, with a focus on the hippocampus and
		  posterior cingulate regions. Participants' cognitive
		  changes were also assessed before and after training. We
		  found that PRT but not CCT significantly improved global
		  cognition (F(90)=4.1, P{\textless}0.05) as well as expanded
		  gray matter in the posterior cingulate (Pcorrected
		  {\textless}0.05), and these changes were related to each
		  other (r=0.25, P=0.03). PRT also reversed progression of
		  white matter hyperintensities, a biomarker of
		  cerebrovascular disease, in several brain areas. In
		  contrast, CCT but not PRT attenuated decline in overall
		  memory performance (F(90)=5.7, P{\textless}0.02), mediated
		  by enhanced functional connectivity between the hippocampus
		  and superior frontal cortex. Our findings indicate that
		  physical and cognitive training depend on discrete neuronal
		  mechanisms for their therapeutic efficacy, information that
		  may help develop targeted lifestyle-based preventative
		  strategies.Molecular Psychiatry advance online publication,
		  22 March 2016; doi:10.1038/mp.2016.19.},
  annote	= {NULL},
  author	= {Suo, C and Singh, M F and Gates, N and Wen, W and Sachdev,
		  P and Brodaty, H and Saigal, N and Wilson, G C and
		  Meiklejohn, J and Singh, N and Baune, B T and Baker, M and
		  Foroughi, N and Wang, Y and Mavros, Y and Lampit, A and
		  Leung, I and Valenzuela, M J},
  doi		= {10.1038/mp.2016.19},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Suo et al. - 2016 - Therapeutically
		  relevant structural and functional mechanisms triggered by
		  physical and cognitive exercise.pdf:pdf},
  isbn		= {1476-5578},
  issn		= {1476-5578 (Electronic)},
  journal	= {Molecular psychiatry},
  keywords	= {cognitive training,exercise,fmri,mri,rest},
  mendeley-tags	= {cognitive training,exercise,fmri,mri,rest},
  number	= {January},
  pages		= {1--10},
  pmid		= {27001615},
  publisher	= {Nature Publishing Group},
