  journal	= {Frontiers in behavioral neuroscience},
  keywords	= {ai,anterior insula,anterior insula (A,anxiety,dacc,dorsal
		  anterior cingulate cortex,fmri,rest,salience
		  network,salience network (SN),sn},
  mendeley-tags	= {fmri,rest,salience network},
  number	= {January},
  pages		= {350},
  pmid		= {26834594},
  title		= {{Decreased Intra- and Inter-Salience Network Functional
		  Connectivity is Related to Trait Anxiety in Adolescents.}},
