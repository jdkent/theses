@Article{	  haris2011,
  abstract	= {In the current study, we aim to measure T1rho (T
		  (1$\rho$)) in the hippocampus in the brain of control,
		  Alzheimer's disease (AD), Parkinson's disease (PD), and PD
		  patients with dementia (PDD), and to determine efficacy of
		  T (1$\rho$) in differentiating these cohorts. With informed
		  consent, 53 AD patients, 62 PD patients, 11 PDD patients,
		  and 46 age-matched controls underwent a standardized
		  clinical assessment including mini-mental state examination
		  (MMSE) and brain T (1$\rho$) MRI on a 1.5-T
		  clinical-scanner. T(1$\rho$) maps were generated by fitting
		  each pixel's intensity as a function of the spin-lock pulse
		  duration. In control, AD, PD and PDD, mean ± SE T
		  (1$\rho$) values in the right hippocampus (RH) were 92.15
		  ± 2.00, 99.65 ± 1.98, 85.68 ± 1.87, 102.47 ± 4.66 ms
		  while in the left hippocampus (LH) these values were 90.16
		  ± 1.82, 99.53 ± 1.91, 84.33 ± 2.03, 95.33 ± 4.64 ms.
		  Significant difference for both RH and LH T (1$\rho$)
		  across the groups (p {\textless} 0.001) was observed. Both
		  RH and LH T (1$\rho$) were significantly increased in AD
		  compared to control (p = 0.034, p = 0.001) and PD (p
		  {\textless} 0.001, p {\textless} 0.001). In control, both
		  RH and LH T (1$\rho$) values were significantly increased
		  compared to PD (p = 0.031, p = 0.027) while compared to PDD
		  only the RH T (1$\rho$) value was significantly decreased
		  (p = 0.043). Both RH and LH T (1$\rho$) values in PD were
		  significantly lower than PDD (p = 0.004, p = 0.032). No
		  significant correlation between the T (1$\rho$) and age as
		  well as between T (1$\rho$) and MMSE scores was observed.
		  The serial measurement of T(1$\rho$) in both AD and PD may
		  provide the nature of disease progression and may
		  contribute to their early diagnosis.},
  author	= {Haris, Mohammad and Singh, Anup and Cai, Kejia and
		  Davatzikos, Christos and Trojanowski, John Q. and Melhem,
		  Elias R. and Clark, Christopher M. and Borthakur, Arijitt},
  doi		= {10.1007/s00415-010-5762-6},
  file		= {:media/Data/Downloads/Haris2011{\_}Article{\_}T1rhoT1$\rho$MRImagingInAlzheimerDi.pdf:pdf},
  issn		= {03405354},
  journal	= {Journal of Neurology},
  keywords	= {Alzheimer's disease,Hippocampus,Parkinson
		  disease,Parkinson disease with dementia,T1rho MRI},
  number	= {3},
  pages		= {380--385},
  pmid		= {20924593},
  title		= {{T1rho (T1$\rho$) MR imaging in Alzheimer' disease and
		  Parkinson's disease with and without dementia}},
  volume	= {258},
  year		= {2011}
}

