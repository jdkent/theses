@Article{	  schnell2015,
  abstract	= {One of the major hurdles I face as the head of a
		  computational biology laboratory is convincing my research
		  team—particularly those pursuing exclusively mathematical
		  and computational modeling—that they need to keep a
		  laboratory notebook. There seems to be a misconception in
		  the computational biology community that a lab notebook is
		  only useful for recording experimental protocols and their
		  results. A lab notebook is much more than that. It is an
		  orga- nizational tool and memory aid, which serves as the
		  primary record of scientific research and activity for all
		  scientists. It also serves as a legal record of ownership
		  of the ideas and results obtained by a scientist. Here, I
		  present the best practices (summarized as ten rules) for
		  keeping a lab notebook in computational biology, for
		  scientists pursuing exclusively “dry” research.},
  annote	= {Use code for all automated steps, enhance with
		  README.md/NOTES.md for all manual labor/steps
		  
		  all notes are kept in the directory in which they are used.
		  
		  if you think the code is going to be useful in more than
		  one place, abstract it out and have the function named.
		  
		  Separation of thoughts and activities, the steps to
		  complete something should be separate from thoughts about
		  something.
		  
		  For the thoughts section, use jekyll blogging structure
		  
		  jekyll: https://github.com/tlnagy/jekyll-lab-notebook
		  
		  maybe elab: https://www.elabftw.net/
		  
		  underlying issues: 1) how do we keep analyses reproducible?
		  2) how do we take notes on thoughts about data or theories?
		  
		  components: data producers (people entering data/running
		  participants) data consumers (people organizing/analyzing
		  data)
		  
		  principles: - keeping the analyses close to the data -
		  referencing analyses when necessary in other places - make
		  code easy to run - document steps through code that you can
		  run from any machine (code says what exactly you are doing
		  better than language)},
  author	= {Schnell, Santiago},
  doi		= {10.1371/journal.pcbi.1004385},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Schnell - 2015 - Ten Simple Rules for a
		  Computational Biologist's Laboratory Notebook.pdf:pdf},
  isbn		= {1553-7358 (Electronic)$\backslash$r1553-734X (Linking)},
  issn		= {15537358},
  journal	= {PLoS Computational Biology},
  number	= {9},
  pages		= {5--9},
  pmid		= {26356732},
  title		= {{Ten Simple Rules for a Computational Biologist's
		  Laboratory Notebook}},
  volume	= {11},
  year		= {2015}
}

