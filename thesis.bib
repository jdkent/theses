@Article{	  bland2012,
  abstract	= {The study of uncertainty in decision-making is receiving
		  greater attention in the fields of cognitive and
		  computational neuroscience. Several lines of evidence are
		  beginning to elucidate different variants of uncertainty.
		  Particularly, risk, ambiguity, and expected and unexpected
		  forms of uncertainty are well articulated in the
		  literature. In this article we review both empirical and
		  theoretical evidence arguing for the potential distinction
		  between three forms of uncertainty; expected uncertainty,
		  unexpected uncertainty, and volatility. Particular
		  attention will be devoted to exploring the distinction
		  between unexpected uncertainty and volatility which has
		  been less appreciated in the literature. This includes
		  evidence mainly from neuroimaging, neuromodulation, and
		  electrophysiological studies. We further address the
		  possible differentiation of cognitive control mechanisms
		  used to deal with these forms of uncertainty. Finally, we
		  explore whether the dual modes of control theory provides a
		  theoretical framework for understanding the distinction
		  between unexpected uncertainty and volatility.},
  author	= {Bland, Amy R. and Schaefer, Alexandre},
  doi		= {10.3389/fnins.2012.00085},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Bland, Schaefer - 2012 - Different
		  varieties of uncertainty in human decision-making.pdf:pdf},
  isbn		= {1662-453X (Electronic)$\backslash$n1662-453X (Linking)},
  issn		= {16624548},
  journal	= {Frontiers in Neuroscience},
  keywords	= {Decision-making,Uncertainty,Unexpected
		  uncertainty,Volatility,cognitive control,review,theory},
  mendeley-tags	= {cognitive control,review,theory},
  number	= {JUN},
  pages		= {1--11},
  pmid		= {22701401},
  title		= {{Different varieties of uncertainty in human
		  decision-making}},
  volume	= {6},
  year		= {2012}
}

