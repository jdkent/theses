  isbn		= {0000000000000},
  issn		= {15300315},
  journal	= {Medicine and Science in Sports and Exercise},
  keywords	= {COGNITION,CYCLING,INHIBITORY CONTROL,PHYSICAL
		  ACTIVITY,WORKING MEMORY,cognitive
		  control,exercise,flanker,nback,task},
  mendeley-tags	= {cognitive control,exercise,flanker,nback,task},
  number	= {7},
