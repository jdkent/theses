  keywords	= {Anatomical connectivity,Connectivity,DTI,Diffusion tensor
		  imaging,Functional connectivity,Resting-state
		  connectivity,Resting-state fMRI,White
		  matter,dwi,fMRI,fmri,rest},
  mendeley-tags	= {dwi,fmri,rest},
  number	= {10},
  pages		= {3127--3141},
