@Article{	  colcombe2003,
  abstract	= {A meta-analytic study was conducted to examine the
		  hypothe- sis that aerobic fitness training enhances the
		  cognitive vitality of healthy but sedentary older adults.
		  Eighteen intervention studies published be- tween 1966 and
		  2001 were entered into the analysis. Several theoretically
		  and practically important results were obtained. Most
		  important, fitness training was found to have robust but
		  selective benefits for cognition, with the largest
		  fitness-induced benefits occurring for executive-control
		  pro- cesses. The magnitude of fitness effects on cognition
		  was also moderated by a number of programmatic and
		  methodological factors, including the length of the
		  fitness-training intervention, the type of the
		  intervention, the duration of training sessions, and the
		  gender of the study participants. The results are discussed
		  in terms of recent neuroscientific and psycholog- ical data
		  that indicate cognitive and neural plasticity is maintained
		  throughout the life span. The},
  annote	= {speed hypothesis: simple reaction time or finger tapping
		  
		  visuo-spatial hypothesis: visuo-spatial processes are more
		  impacted than verbal skills
		  
		  controlled-processing hypothesis: controlled processes
		  become automatic with consistent practice
		  
		  executive-control hypothesis: coordination, inhibition,
		  scheduling, planning, and working memory.},
  archiveprefix	= {arXiv},
  arxivid	= {10.1111/1467-9280.t01-1-01430},
  author	= {Colcombe, S. and Kramer, A.F.},
  doi		= {10.1111/1467-9280.t01-1-01430},
  eprint	= {1467-9280.t01-1-01430},
  file		= {:media/Data/Downloads/Colcombe-2003-Fitness effects on the
		  cognitive.pdf:pdf},
  isbn		= {0956-7976 (Print)},
  issn		= {0956-7976},
  journal	= {Psychological Science},
  pages		= {125},
  pmid		= {12661673},
  primaryclass	= {10.1111},
  title		= {{Fitness effects on the cognitive function of older
		  adults}},
  url		= {http://pss.sagepub.com/content/14/2/125.full.pdf+html},
  volume	= {14},
  year		= {2003}
}

