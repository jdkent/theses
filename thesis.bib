@Article{	  moe2016,
  abstract	= {ABSTRACT Displayed enthusiasm has been shown to relate to
		  intrinsic motivation, vitality, and positive affect, but
		  its effects on recall performance and time estimation have
		  not yet been explored. This research aimed at studying the
		  effects of a delivery style characterised by High
		  Enthusiasm (HE) on recall, time estimation, and intrinsic
		  motivation. In line with previous studies, effects on
		  intrinsic motivation were expected. In addition, higher
		  recall and lower time estimations were hypothesised. In two
		  experiments, participants assigned to a HE condition or to
		  a normal reading control condition listened to a narrative
		  and to a descriptive passage. Then, they were asked to rate
		  perceived time, enthusiasm, pleasure, interest, enjoyment
		  and curiosity, before writing a free recall. Experiment 1
		  showed that in the HE condition, participants recalled
		  more, were more intrinsically motivated, and expressed
		  lower time estimations compared to the control condition.
		  Experiment 2 confirmed the positive effects of HE reading
		  compared to normal reading, using different passages and a
		  larger sample.},
  annote	= {Enthusiasm causes: positive emotional experience
		  expectation of reward fulfilment of motives such as
		  autonomy, competence, and relatedness.
		  
		  Enthusiasm characteristics style of delivery of information
		  display of interes a pursuit of goals high levels of
		  engagement arousal and excitement set of non-verbal
		  behaviors and gestures
		  
		  gap: investigating enthusiasm on recall
		  
		  hypothesis: more enthusiasm in the presenter will lead to
		  better recall in the recipients
		  
		  specific quesion and experiment: effects of displayed
		  enthusiasm on text recall, intrinsic motivation and time
		  estimation
		  
		  an enthusiastic delivering style favors recall, affects
		  pleasure and motivates interest,},
  author	= {Mo{\`{e}}, Angelica},
  doi		= {10.1080/02699931.2015.1061480},
  file		= {:media/Data/Downloads/Does displayed enthusiasm favour
		  recall intrinsic motivation and time estimation.pdf:pdf},
  issn		= {14640600},
  journal	= {Cognition and Emotion},
  keywords	= {Enthusiasm,intrinsic motivation,positive affect,recall},
  number	= {7},
  pages		= {1361--1369},
  publisher	= {Taylor {\&} Francis},
  title		= {{Does displayed enthusiasm favour recall, intrinsic
		  motivation and time estimation?}},
  volume	= {30},
  year		= {2016}
}

###Article{	  moe2016,
