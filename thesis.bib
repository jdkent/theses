  journal	= {Magnetic resonance in medicine : official journal of the
		  Society of Magnetic Resonance in Medicine / Society of
		  Magnetic Resonance in Medicine},
  keywords	= {3t,longitudinal relaxa-,magnetization
		  transfer,mt,other,pd,quantitative,r 1,r 2,relaxometry,t 1,t
		  2,tion,transverse relaxation,water content},
  mendeley-tags	= {other},
