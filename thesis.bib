  issn		= {0036-8075},
  journal	= {Proceedings of SPIE--the International Society for Optical
		  Engineering},
  keywords	= {fmri,livelihood
		  diversification,maasai,method,migration,mvpa,pastoralism,tanzania},
  mendeley-tags	= {fmri,method,mvpa},
  number	= {4},
  pages		= {389--400},
  pmid		= {25792328},
  title		= {{HHS Public Access}},
