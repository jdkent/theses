@Article{	  siman-tov2017,
  abstract	= {As the world ages, it becomes urgent to unravel the
		  mechanisms underlying brain aging and find ways of
		  intervening with them. While for decades cognitive aging
		  has been related to localized brain changes, growing
		  attention is now being paid to alterations in distributed
		  brain networks. Functional connectivity magnetic resonance
		  imaging (fcMRI) has become a particularly useful tool to
		  explore large-scale brain networks; yet, the temporal
		  course of connectivity lifetime changes has not been
		  established. Here, an extensive cross-sectional sample
		  (21-85 years old, N = 887) from a public fcMRI database was
		  used to characterize adult lifespan connectivity dynamics
		  within and between seven brain networks: the default mode,
		  salience, dorsal attention, fronto-parietal control,
		  auditory, visual and motor networks. The entire cohort was
		  divided into young (21-40 years, mean ± SD: 25.5 ± 4.8, n
		  = 543); middle-aged (41-60 years, 50.6 ± 5.4, n = 238);
		  and old (61 years and above, 69.0 ± 6.3, n = 106)
		  subgroups. Correlation matrices as well as a mixed model
		  analysis of covariance indicated that within high-order
		  cognitive networks a considerable connectivity decline is
		  already evident by middle adulthood. In contrast, a motor
		  network shows increased connectivity in middle adulthood
		  and a subsequent decline. Additionally, alterations in
		  inter-network interactions are noticeable primarily in the
		  transition between young and middle adulthood. These
		  results provide evidence that aging-related neural changes
		  start early in adult life.},
  annote	= {Intro: A large dataset that includes analysis of all major
		  ICNs, both between and within network interactions.
		  
		  had two analyses: one with 887 adults one with 318 adults
		  (to size match the groups)
		  
		  included various conditions (eyes open/eyes closed, left
		  handed/right handed), but did not include that information
		  in the model?},
  author	= {Siman-Tov, Tali and Bosak, Noam and Sprecher, Elliot and
		  Paz, Rotem and Eran, Ayelet and Aharon-Peretz, Judith and
		  Kahn, Itamar},
  doi		= {10.3389/fnagi.2016.00330},
  file		= {:media/Data/Downloads/fnagi-08-00330 (1).pdf:pdf},
  issn		= {16634365},
  journal	= {Frontiers in Aging Neuroscience},
  keywords	= {Aging,Brain networks,Functional
		  connectivity,Lifespan,MRI,Resting state,aging,default mode
		  network,dorsal attention network,fmri,frontoparietal
		  network,motor network,rest,salience network,visual
		  network},
  mendeley-tags	= {aging,default mode network,dorsal attention
		  network,fmri,frontoparietal network,motor
		  network,rest,salience network,visual network},
  number	= {JAN},
  pages		= {1--17},
  pmid		= {28119599},
  title		= {{Early age-related functional connectivity decline in
		  high-order cognitive networks}},
  volume	= {8},
  year		= {2017}
}

