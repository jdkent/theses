@Article{	  rollo2009,
  abstract	= {Aging encompasses life itself so understanding requires
		  frameworks that forge unity amidst complexity. The free
		  radical theory of aging is one example. The original focus
		  on damage was augmented recently by appreciation that
		  reactive oxygen and nitrogen species are essential to
		  normal signaling and cell function. This paradigm is
		  currently undergoing an explosive expansion fueled by the
		  discovery that regulatory organization is a merry-go-round
		  of redox cycling seamlessly fused to endogenous clocks.
		  This might best be described as an "Electroplasmic Cycle."
		  This is certainly applicable to dopaminergic neurons with
		  their exceptional metabolic, electrical and rhythmic
		  properties. Here I review normal aging of dopamine systems
		  to highlight them as a valuable model. I then examine the
		  possible integration of free radical and ion channel
		  theories of aging. Finally, I incorporate clocks and
		  explore the multifaceted implications of electroplasmic
		  cycles with special emphasis on dopamine.},
  annote	= {DA degradation generates free radicals by monoamine
		  oxidases there are also potentially toxic DA metabolic
		  products, relatively low levels of antioxidant defenses in
		  brain (especially catalase) low expression of growth
		  factors in regions of high DA vulnerability high levels of
		  iron associated with the SN with possible dysregulation of
		  heme exceptional metabolic rates of DA neurons
		  (particularlly SN) abundance of oxidizable unsaturated
		  fatty acids and DA tyrosine hydroxylase is inhibited by
		  oxidation},
  author	= {Rollo, C. David},
  doi		= {10.1007/s11064-008-9858-7},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Rollo - 2009 - Dopamine and aging
		  Intersecting facets.pdf:pdf},
  isbn		= {1573-6903 (Electronic)$\backslash$r0364-3190 (Linking)},
  issn		= {03643190},
  journal	= {Neurochemical Research},
  keywords	= {Aging,Clocks,Dopamine,Electroplasmic cycle,Evolutionary
		  theory,Free radicals,Ion
		  channels,Longevity,Redox,Regulation,aging,dopamine,review,theory},
  mendeley-tags	= {aging,dopamine,review,theory},
  number	= {4},
  pages		= {601--629},
  pmid		= {18841466},
  title		= {{Dopamine and aging: Intersecting facets}},
  volume	= {34},
  year		= {2009}
}

