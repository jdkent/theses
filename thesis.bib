  journal	= {BioMed research international},
  keywords	= {aging,dwi,fmri,rest},
  mendeley-tags	= {aging,dwi,fmri,rest},
  month		= {jan},
  pages		= {947252},
  pmid		= {24971361},
  title		= {{Intrinsic functional connectivity networks in healthy
		  elderly subjects: a multiparametric approach with
