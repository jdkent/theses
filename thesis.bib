@Article{	  yehene2008,
  abstract	= {The performance of patients with lesions involving the
		  basal ganglia (BG) was compared to that of patients with
		  prefrontal (PFC) lesions, thalamic (TH) lesions, and
		  age-matched controls in order to examine the specific role
		  of the BG within the frontal-subcortical circuits (FSCC) in
		  task switching. All the BG patients and none of the other
		  participants showed a marked increase in error rate in
		  incongruent trials where correct responses depended upon
		  the choice of the correct task rule. Some BG patients erred
		  in failing to switch tasks and others failed despite their
		  attempt to switch tasks. Additionally, reaction time
		  results indicate abnormal response repetition effects among
		  the BG patients; failure in benefiting from advance task
		  information among all the patients; and increased task
		  mixing costs following PFC lesions. The authors conclude
		  that although the frontal-subcortical circuits jointly
		  determine some behaviors (such as benefiting from
		  preparation), the BG play a unique role within the FSCC in
		  action selection and/or the inhibition of irrelevant
		  information.},
  author	= {Yehene, Einat and Meiran, Nachshon and Soroker, Nachum},
  doi		= {10.1162/jocn.2008.20077},
  file		= {:media/Data/Downloads/jocn{\%}2E2008{\%}2E20077.pdf:pdf},
  isbn		= {0898-929X (Print) 0898-929X (Linking)},
  issn		= {0898-929X},
  journal	= {Journal of cognitive neuroscience},
  keywords	= {cognitive control,lesion,task,task-switch},
  mendeley-tags	= {cognitive control,lesion,task,task-switch},
  number	= {6},
  pages		= {1079--1093},
  pmid		= {18211234},
  title		= {{Basal ganglia play a unique role in task switching within
		  the frontal-subcortical circuits: evidence from patients
		  with focal lesions.}},
  volume	= {20},
  year		= {2008}
}

