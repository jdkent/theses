@Article{	  morehead2019,
  abstract	= {Many students use laptops to take notes in classes, but
		  does using them impact later test performance? In a
		  high-profile investigation comparing note-taking writing on
		  paper versus typing on a laptop keyboard, Mueller and
		  Oppenheimer (Psychological Science, 25,1159– 1168, 2014)
		  concluded that taking notes by longhand is superior. We
		  conducted a direct replication ofMueller and Oppenheimer
		  (2014) and extended their work by including groups who took
		  notes using eWriters and who did not take notes. Some
		  trends suggested longhand superiority; however, performance
		  did not consistently differ between any groups (experiments
		  1 and 2), including a group who did not take notes
		  (experiment 2). Group differences were further decreased
		  after students studied their notes (experiment 2). A
		  meta-analysis (combining direct replications) of test
		  performance revealed small (nonsignificant) effects
		  favoring long- hand. Based on the present outcomes and
		  other available evidence, concluding which method is
		  superior for improving the functions of note-taking seems
		  premature.},
  annote	= {premise: students are taking notes on their laptops.
		  
		  the five whys: - ask them why about their answer for a
		  couple levels
		  
		  course evals drinking game
		  
		  lifespan story weaving
		  
		  experience with laptops may have an influence on well the
		  notes are taken
		  
		  the ecological validty of the study is limited. tests are
		  too close to the lecture.
		  
		  don't share slides, but share the figures
		  
		  empirical study of sharing notes versus not sharing notes.
		  
		  post some videos for some lectures, and see performance on
		  the topics},
  author	= {Morehead, Kayla and Dunlosky, John and Rawson, Katherine
		  A.},
  doi		= {10.1007/s10648-019-09468-2},
  file		= {:media/Data/Downloads/Morehead2019{\_}Article{\_}HowMuchMightierIsThePenThanThe.pdf:pdf},
  issn		= {1573336X},
  journal	= {Educational Psychology Review},
  keywords	= {Encoding,Laptop,Longhand,Note-taking,Storage,eWriter},
  title		= {{How Much Mightier Is the Pen than the Keyboard for
		  Note-Taking? A Replication and Extension of Mueller and
		  Oppenheimer (2014)}},
  year		= {2019}
}

@Article{	  moret-tatay2017,
  abstract	= {The study of reaction times and their underlying cognitive
		  processes is an important field in Psychology. Reaction
		  times are usually modeled through the ex-Gaussian
		  distribution, because it provides a good fit to multiple
		  empirical data. The complexity of this distribution makes
		  the use of computational tools an essential element in the
		  field. Therefore, there is a strong need for efficient and
		  versatile computational tools for the research in this
		  area. In this manuscript we discuss some mathematical
		  details of the ex-Gaussian distribution and apply the
		  ExGUtils package, a set of functions and numerical tools,
		  programmed for python, developed for numerical analysis of
		  data involving the ex-Gaussian probability density. In
		  order to validate the package, we present an extensive
		  analysis of fits obtained with it, discuss advantages and
		  differences between the least squares and maximum
		  likelihood methods and quantitatively evaluate the goodness
		  of the obtained fits (which is usually an overlooked point
		  in most literature in the area). The analysis done allows
		  one to identify outliers in the empirical datasets and
		  criteriously determine if there is a need for data trimming
		  and at which points it should be done.},
  archiveprefix	= {arXiv},
  arxivid	= {1707.05759},
  author	= {Moret-Tatay, Carmen and Gamermann, Daniel and
		  Navarro-Pardo, Esperanza and de C{\'{o}}rdoba, Pedro
		  Fernandez},
  eprint	= {1707.05759},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Moret-Tatay et al. - 2017 - ExGUtils A
		  python package for statistical analysis with the
		  ex-gaussian probability density.pdf:pdf},
  keywords	= {method,python,reactiontime},
  mendeley-tags	= {method,python,reactiontime},
  pages		= {1--18},
  title		= {{ExGUtils: A python package for statistical analysis with
		  the ex-gaussian probability density}},
  url		= {http://arxiv.org/abs/1707.05759},
  year		= {2017}
}

