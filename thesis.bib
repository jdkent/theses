  isbn		= {1095-9572 (Electronic)$\backslash$n1053-8119 (Linking)},
  issn		= {10538119},
  journal	= {NeuroImage},
  keywords	= {FMRI,Midbrain,Physiological noise,Registration,Superior
		  colliculi,fmri,method,midbrain,rest,task},
  mendeley-tags	= {fmri,method,midbrain,rest,task},
  number	= {2},
  pages		= {1230--1238},
