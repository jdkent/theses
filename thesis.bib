  issn		= {18733514},
  journal	= {Neuropsychologia},
  keywords	= {AX-CPT,Inhibition,Monitoring,N2.,P3,ax-cpt,cognitive
		  control,eeg,proactive,reactive,task},
  mendeley-tags	= {ax-cpt,cognitive control,eeg,proactive,reactive,task},
  pages		= {157--169},
  pmid		= {25448864},
  publisher	= {Elsevier},
  title		= {{Bilingualism modulates dual mechanisms of cognitive
		  control: Evidence from ERPs}},
