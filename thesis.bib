@Article{	  gray2011,
  abstract	= {Patients in persistent vegetative state (PVS) may be
		  biologically alive, but these experiments indicate that
		  people see PVS as a state curiously more dead than dead.
		  Experiment 1 found that PVS patients were perceived to have
		  less mental capacity than the dead. Experiment 2 explained
		  this effect as an outgrowth of afterlife beliefs, and the
		  tendency to focus on the bodies of PVS patients at the
		  expense of their minds. Experiment 3 found that PVS is also
		  perceived as "worse" than death: people deem early death
		  better than being in PVS. These studies suggest that people
		  perceive the minds of PVS patients as less valuable than
		  those of the dead - ironically, this effect is especially
		  robust for those high in religiosity. {\textcopyright} 2011
		  Elsevier B.V.},
  author	= {Gray, Kurt and {Anne Knickman}, T. and Wegner, Daniel M.},
  doi		= {10.1016/j.cognition.2011.06.014},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Gray, Anne Knickman, Wegner - 2011 -
		  More dead than dead Perceptions of persons in the
		  persistent vegetative state.pdf:pdf},
  isbn		= {0010-0277},
  issn		= {00100277},
  journal	= {Cognition},
  keywords	= {Dualism,End of life decisions,Medical ethics,Mind
		  perception,Morality,other},
  mendeley-tags	= {other},
  number	= {2},
  pages		= {275--280},
  pmid		= {21798528},
  publisher	= {Elsevier B.V.},
  title		= {{More dead than dead: Perceptions of persons in the
		  persistent vegetative state}},
