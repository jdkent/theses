@Article{	  shomstein2013,
  abstract	= {The contribution of rewarded actions to automatic
		  attentional selection remains obscure. We hypothesized that
		  some forms of automatic orienting, such as object-based
		  selection, can be completely abandoned in favor of a
		  reward-maximizing strategy. In the two experiments reported
		  here, we presented identical visual stimuli to observers
		  while manipulating what was being rewarded (targets in
		  different locations from the cue or in random object
		  locations) and the type of reward received (money or
		  points). We found that reward alone, not the objects,
		  guides attentional selection and thus entirely predicts
		  behavior. These results suggest that guidance of selective
		  attention, although automatic, is flexible and can be
		  adjusted in accordance with external nonsensory
		  reward-based factors.},
  author	= {Shomstein, Sarah and Johnson, Jacoba},
  doi		= {10.1177/0956797613490743},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Shomstein, Johnson - 2013 - Shaping
		  attention with reward effects of reward on space- and
		  object-based selection.pdf:pdf},
  isbn		= {0956797613490},
  issn		= {1467-9280},
  journal	= {Psychological science},
  keywords	= {other},
  mendeley-tags	= {other},
  number	= {12},
  pages		= {2369--78},
  pmid		= {24121412},
  title		= {{Shaping attention with reward: effects of reward on
		  space- and object-based selection.}},
  url		= {http://pss.sagepub.com/content/early/2013/10/11/0956797613490743},
  volume	= {24},
  year		= {2013}
}

