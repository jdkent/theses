@Article{	  cabeza2012b,
  abstract	= {Argues for the validity of a representational research
		  framework to understand human prefrontal cortex (HPFC)
		  functioning. This framework suggests that by postulating
		  the form of the various units of representation (in
		  essence, the elements of memory) stored in PFC, it will be
		  much easier to derive clear and testable hypotheses that
		  will enable rejection or validation of this and other
		  frameworks. The set of HPFC representational units alluded
		  to above is labeled as a structured event complex (SEC). In
		  this chapter the author details hypotheses about the SEC's
		  representational structure and features and attempts to
		  distinguish the SEC framework from other cognitive models
		  of HPFC function. Before doing so, the author briefly
		  summarizes the key elements of the biology and structure of
		  the HPFC, evidence of its general role in cognition based
		  on convergent evidence from lesion and neuroimaging
		  studies, and some key models postulating the functions of
		  the HPFC. The author then briefly argues for a rationale
		  that specifies why there must be representational knowledge
		  stored in the PFC, presents a short primer on the SEC
		  framework he has adapted, and finally offers some
		  suggestions about future directions for research of the
		  HPFC functions using the SEC framework.},
  author	= {Cabeza, R and Dennis, N a},
  doi		= {10.1093/acprof:oso/9780195134971.001.0001},
