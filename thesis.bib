@Article{	  mather2016,
  abstract	= {Research on cognitive aging has focused on how decline in
		  various cortical and hippocampal regions influence
		  cognition. However, brainstem regions play essential
		  modulatory roles, and new evidence suggests that, among
		  these, the integrity of the locus coeruleus
		  (LC)-norepinephrine (NE) system plays a key role in
		  determining late-life cognitive abilities. The LC is
		  especially vulnerable to toxins and infection and is often
		  the first place Alzheimer's-related pathology appears, with
		  most people showing at least some tau pathology by their
		  mid-20s. On the other hand, NE released from the LC during
		  arousing, mentally challenging, or novel situations helps
		  to protect neurons from damage, which may help to explain
		  how education and engaging careers prevent cognitive
		  decline in later years. In late life, lower LC neural
		  density is associated with cognitive decline.Because of the
		  long unmyelinated axons of its neurons, high exposure to
		  blood flow, and location adjacent to the 4th ventricle, the
		  LC is especially vulnerable to toxins.The tau pathology
		  precursor of Alzheimer's disease emerges in the LC by early
		  adulthood in most people. However, the pathology typically
		  spreads slowly, and only some end up with clinically
		  evident Alzheimer's disease.Norepinephrine helps to protect
		  neurons from factors that accelerate Alzheimer's disease,
		  such as inflammation and excitotoxicity.Education and
		  engaging careers produce late-life 'cognitive reserve' or
		  effective brain performance despite encroaching pathology.
		  Activation of the LC-NE system by novelty and mental
		  challenge throughout life may contribute to cognitive
		  reserve.},
  annote	= {LC neuron count may not change with aging.},
  author	= {Mather, Mara and Harley, Carolyn W.},
  doi		= {10.1016/j.tics.2016.01.001},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Mather, Harley - 2016 - The Locus
		  Coeruleus Essential for Maintaining Cognitive Function and
		  the Aging Brain.pdf:pdf},
  isbn		= {1364-6613},
  issn		= {1879307X},
  journal	= {Trends in Cognitive Sciences},
  keywords	= {aging,midbrain,review,theory},
  mendeley-tags	= {aging,midbrain,review,theory},
  number	= {3},
  pages		= {214--226},
  pmid		= {26895736},
  publisher	= {Elsevier Ltd},
  title		= {{The Locus Coeruleus: Essential for Maintaining Cognitive
		  Function and the Aging Brain}},
  url		= {http://dx.doi.org/10.1016/j.tics.2016.01.001},
  volume	= {20},
  year		= {2016}
}

