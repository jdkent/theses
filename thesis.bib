  issn		= {10974172},
  journal	= {Cell},
  keywords	= {brain,depression,electricity,ketamine,networks,oscillations,spatiotemporal
		  dynamics,stress},
  number	= {1},
  pages		= {166--180.e14},
  publisher	= {Elsevier},
  title		= {{Brain-wide Electrical Spatiotemporal Dynamics Encode
		  Depression Vulnerability}},
  url		= {http://dx.doi.org/10.1016/j.cell.2018.02.012},
