  doi		= {10.1016/j.neuroimage.2004.09.007},
  file		= {:media/Data/Downloads/Egner{\&}Hirsch2005.pdf:pdf;:media/Data/Downloads/nihms128499.pdf:pdf;:media/Data/Downloads/art{\%}3A10.1007{\%}2Fs00429-015-1088-6.pdf:pdf},
  isbn		= {0042901510},
  issn		= {18632661},
  journal	= {Brain Structure and Function},
  keywords	= {Anxiety vulnerability,Behavioral
		  inhibition,Cerebellum,Cognitive control,Conflict
		  adaptation,Executive network,Psychophysiological
		  interaction,Resting-state functional connectivity,Stroop
