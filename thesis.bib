  journal	= {Cerebral cortex},
  keywords	= {brain network,cognitive
		  control,connectivity,flanker,fmri,human,sst,task,temporal
		  dependence},
  mendeley-tags	= {cognitive control,flanker,fmri,sst,task},
  number	= {May},
  pages		= {1--14},
  pmid		= {25778346},
  title		= {{Causal Interactions Within a Frontal-Cingulate-Parietal
		  Network During Cognitive Control: Convergent Evidence from
		  a Multisite-Multitask Investigation.}},
