  journal	= {Proceedings of the National Academy of Sciences of the
		  United States of America},
  keywords	= {other},
  mendeley-tags	= {other},
  number	= {49},
  pages		= {19571--19576},
  pmid		= {18073185},
  title		= {{A structural basis for enhancement of long-term
