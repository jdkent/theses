@Article{	  figdor2013,
  abstract	= {This paper argues that the cognitive neuroscientific use
		  of ordinary mental terms to report research results and
		  draw implications can contribute to public confusion and
		  misunderstanding regarding neuroscience results. This
		  concern is raised at a time when cognitive neuroscientists
		  are increasingly required by funding agencies to link their
		  research to specific results of public benefit, and when
		  neuroethicists have called for greater attention to public
		  communication of neuroscience. The paper identifies an
		  ethical dimension to the problem and presses for greater
		  sensitivity and responsibility among neuroscientists
		  regarding their use of such terms.},
