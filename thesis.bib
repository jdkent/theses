@Article{	  truog2014,
  abstract	= {We seek to change the conversation about brain death by
		  highlighting the distinction between brain death as a
		  biological concept versus brain death as a legal status.
		  The fact that brain death does not cohere with any
		  biologically plausible definition of death has been known
		  for decades. Nevertheless, this fact has not threatened the
		  acceptance of brain death as a legal status that permits
		  individuals to be treated as if they are dead. The
		  similarities between "legally dead" and "legally blind"
		  demonstrate how we may legitimately choose bright-line
		  legal definitions that do not cohere with biological
		  reality. Not only does this distinction bring conceptual
		  coherence to the conversation about brain death, but it has
		  practical implications as well. Once brain death is
		  recognized as a social construction not grounded in
		  biological reality, we create the possibility of changing
		  the social construction in ways that may better serve both
		  organ donors and recipients alike.},
  author	= {Truog, Robert D and Miller, Franklin G},
  doi		= {10.1080/15265161.2014.925154},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Truog, Miller - 2014 - Changing the
		  conversation about brain death.pdf:pdf},
  isbn		= {1536-0075 (Electronic) 1526-5161 (Linking)},
  issn		= {1536-0075},
  journal	= {The American journal of bioethics : AJOB},
  keywords	= {brain death,dead donor rule,definition of death,health
		  policy,organ donation,organ transplantation,other},
  mendeley-tags	= {other},
  number	= {8},
  pages		= {9--14},
  pmid		= {25046286},
  title		= {{Changing the conversation about brain death.}},
