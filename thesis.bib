@Article{	  sohn2015,
  abstract	= {The differences in how our brain is connected are often
		  thought to reflect the differences in our individual
		  personalities and cognitive abilities. Individual
		  differences in brain connectivity has long been recognized
		  in the neuroscience community however it has yet to
		  manifest itself in the methodology of resting state
		  analysis. This is evident as previous studies use the same
		  region of interest (ROIs) for all subjects. In this paper
		  we demonstrate that the use of ROIs which are standardized
		  across individuals leads to inaccurate calculations of
		  functional connectivity. We also show that this problem can
		  be addressed by taking an individualized approach by using
		  subject-specific ROIs. Finally we show that ROI selection
		  can affect the way we interpret our data by showing
		  different changes in functional connectivity with ageing.},
  author	= {Sohn, William Seunghyun and Yoo, Kwangsun and Lee,
		  Young-Beom and Seo, Sang Won and Na, Duk L. and Jeong,
		  Yong},
  doi		= {10.3389/fnins.2015.00280},
  file		= {:media/Data/Downloads/fnins-09-00280.pdf:pdf},
  isbn		= {1662-4548 (Print)$\backslash$r1662-453X (Linking)},
  issn		= {1662-453X},
  journal	= {Frontiers in Neuroscience},
  keywords	= {Ageing,Subject-specific ROIs,fmri,graph
		  theory,individ,individual variablity,method,rest,resting
		  state fMRI,subject-specific ROIs},
  mendeley-tags	= {fmri,method,rest},
  number	= {August},
  pages		= {1--10},
  pmid		= {26321904},
  title		= {{Influence of ROI selection on Resting Functional
		  Connectivity: An Individualized Approach for Resting fMRI
		  Analysis}},
