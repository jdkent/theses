  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Hayes, Salat, Verfaellie - 2012 -
		  Default Network Connectivity in Medial Temporal Lobe
		  Amnesia.pdf:pdf},
  isbn		= {0270-6474},
  issn		= {0270-6474},
  journal	= {Journal of Neuroscience},
  number	= {42},
  pages		= {14622--14629a},
  pmid		= {23077048},
  title		= {{Default Network Connectivity in Medial Temporal Lobe
		  Amnesia}},
  url		= {http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.0700-12.2012},
  volume	= {32},
  year		= {2012}
}

