@Article{	  pratte2010,
  abstract	= {Stroop and Simon tasks are logically similar and are often
		  used to investigate cognitive control and inhibition
		  processes. We compare the distributional properties of
		  Stroop and Simon effects with delta plots and find dif-
		  ferent although stable patterns. Stroop effects across a
		  variety of conditions are smallest for fast responses and
		  increase as responses slow. Simon effects across a variety
		  of conditions, however, are largest for fast responses but
		  decrease, and even reverse, as responses slow. We show in
		  three experiments that these diverging patterns hold within
		  participants and even when the stimulus materials are
		  identical across the tasks. These stable dif- ferences in
		  time course serve as bedrock phenomena for building and
		  testing theories of cognitive control and inhibition. The
		  results of two additional experiments suggest that the
		  determinant of time course is not simply whether the
		  distracting information is location.},
  archiveprefix	= {arXiv},
  arxivid	= {NIHMS150003},
  author	= {Pratte, Michael S and Rouder, Jeffrey N and Morey, Richard
		  D and Feng, Chuning},
  doi		= {10.3758/APP},
  eprint	= {NIHMS150003},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Pratte et al. - 2010 - Exploring the
		  differences in distributional properties between Stroop and
		  Simon effects using delta plots.pdf:pdf},
  isbn		= {1943-3921},
  issn		= {1943-3921},
  journal	= {Attention, perception {\&} psychophysics},
  keywords	= {cognitive control,method,simon,stroop,task,theory},
  mendeley-tags	= {cognitive control,method,simon,stroop,task,theory},
  number	= {7},
  pages		= {2013--2025},
  pmid		= {19304639},
  title		= {{Exploring the differences in distributional properties
		  between Stroop and Simon effects using delta plots}},
