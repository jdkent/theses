@Article{	  king2016,
  abstract	= {Recent evidence of unconscious working memory challenges
		  the notion that only visible stimuli can be actively
		  maintained over time. In the present study, we investigated
		  the neural dynamics underlying the maintenance of variably
		  visible stimuli using magnetoencephalography. Subjects had
		  to detect and mentally maintain the orientation of a masked
		  grating. We show that the stimulus is fully encoded in
		  early brain activity independently of visibility reports.
		  However, the presence and orientation of the target are
		  actively maintained throughout the brief retention period,
		  even when the stimulus is reported as unseen. Source and
		  decoding analyses revealed that perceptual maintenance
		  recruits a hierarchical network spanning the early visual,
		  temporal, parietal, and frontal cortices. Importantly, the
		  representations coded in the late processing stages of this
		  network specifically predicted visibility reports. These
		  unexpected results challenge several theories of
		  consciousness and suggest that invisible information can be
		  briefly maintained within the higher processing stages of
		  visual perception.},
  annote	= {invisible stimulus: holding a representation (say a pink
		  elephant) in your mind. Recurrence Theory: invisible
		  stimulus may elicit a feedforward response across the
		  cortical heiarchies, but typically fails to trigger
		  recurrent processing within each processing stage.
		  
		  Global Neuronal Workspace Theory: invisible stimulus may be
		  coded in peripheral cortical modules, but that this
		  unconscious response is too weak to recruit the
		  fronto-parietal networks responsible for the maintenance
		  and global broadcast of the representation across the
		  cortex.
		  
		  Hypothesis/Questions: Is the maintenance of invisible
		  information confined to early sensory stages or broadcast
		  to higher processing stages?
		  
		  Is the maintenance of inivisible information depends on the
		  sustained firing rate of a coding neuronal assembly and/or
		  on the dynamic transmission of info across multiple
		  modules},
  archiveprefix	= {arXiv},
  arxivid	= {http://dx.doi.org/10.1101/040030},
  author	= {King, Jean R{\'{e}}mi and Pescetelli, Niccolo and Dehaene,
		  Stanislas},
  doi		= {10.1016/j.neuron.2016.10.051},
  eprint	= {/dx.doi.org/10.1101/040030},
  file		= {:media/Data/Downloads/Cogntive Neuro Science Journal Club
		  Paper 10{\%}3a31{\%}3a17 (1).pdf:pdf},
  isbn		= {1097-4199 (Electronic)$\backslash$r0896-6273 (Linking)},
  issn		= {10974199},
  journal	= {Neuron},
  keywords	= {attention,backward masking,decoding,journal club,machine
		  learning,magnetoencephalography,perception,subliminal,temporal
		  generalization,visual awareness,working memory},
  mendeley-tags	= {journal club},
  number	= {5},
  pages		= {1122--1134},
  pmid		= {27930903},
  primaryclass	= {http:},
  title		= {{Brain Mechanisms Underlying the Brief Maintenance of Seen
		  and Unseen Sensory Information}},
  volume	= {92},
  year		= {2016}
}

