@Article{	  matsumoto2013,
  abstract	= {Dopamine is essential to cognitive functions. However,
		  despite abundant studies demonstrating that dopamine neuron
		  activity is related to reinforcement and motivation, little
		  is known about what signals dopamine neurons convey to
		  promote cognitive processing. We therefore examined
		  dopamine neuron activity in monkeys performing a delayed
		  matching-to-sample task that required working memory and
		  visual search. We found that dopamine neurons responded to
		  task events associated with cognitive operations. A subset
		  of dopamine neurons were activated by visual stimuli if the
		  monkey had to store the stimuli in working memory. These
		  neurons were located dorsolaterally in the substantia nigra
		  pars compacta, whereas ventromedial dopamine neurons, some
		  in the ventral tegmental area, represented reward
		  prediction signals. Furthermore, dopamine neurons monitored
		  visual search performance, becoming active when the monkey
		  made an internal judgment that the search was successfully
		  completed. Our findings suggest an anatomical gradient of
		  dopamine signals along the dorsolateral-ventromedial axis
		  of the ventral midbrain},
  author	= {Matsumoto, Masayuki and Takada, Masahiko},
  doi		= {10.1016/j.neuron.2013.07.002},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Matsumoto, Takada - 2013 - Distinct
		  Representations of Cognitive and Motivational Signals in
		  Midbrain Dopamine Neurons.pdf:pdf},
  isbn		= {0896-6273},
  issn		= {08966273},
  journal	= {Neuron},
  keywords	= {cognitive control,dopamine,midbrain,task,theory},
  mendeley-tags	= {cognitive control,dopamine,midbrain,task,theory},
  number	= {5},
  pages		= {1011--1024},
