@Article{	  braver2006,
  abstract	= {The facility with which humans perform and shift among a
		  wide variety of cognitive tasks seems to indicate a
		  mechanism for entering into a task-dependent mode or set.
		  However, little is known about the neural systems that
		  subserve task control. A recent neuroimaging study by
		  Dosenbach et al. offers a set of novel methodological tools
		  to examine this issue and uncovers new candidate brain
		  regions for a core system that might implement task sets.},
  annote	= {what are the psycholgoical and neural mechanisms that
		  enable us to encode and maintain task goals and
		  instructions as a 'task set' to ensure high levels of
		  performance across an extended session
		  
		  type of signals that might be markers of a task-set
		  implementation system: start-cue, sustained, error-feedback
		  
		  areas active for all signals: medial frontal cortex (dorsal
		  anterior cingulate cortex and pre-supplementary motor area)
		  
		  ventrolateral frontal cortex (anterior insula and frontal
		  operculum)},
  author	= {Braver, Todd S. and Barch, Deanna M.},
  doi		= {10.1016/j.tics.2006.10.006},
  file		= {:media/Data/Downloads/BraverandBarch2006ExtractingCoreComponents.pdf:pdf},
  isbn		= {1364-6613},
  issn		= {13646613},
  journal	= {Trends in Cognitive Sciences},
  keywords	= {cognitive control,review,theory},
  mendeley-tags	= {cognitive control,review,theory},
  number	= {12},
  pages		= {529--532},
  pmid		= {17071129},
  title		= {{Extracting core components of cognitive control}},
  volume	= {10},
  year		= {2006}
}

