  journal	= {Frontiers in behavioral neuroscience},
  keywords	= {action selection,basal ga,basal ganglia,biased
		  competition,cognitive control,dopamine,dopamine
		  function,dual-process theory of,error
		  processing,performance monitoring,review,theory},
  mendeley-tags	= {cognitive control,review,theory},
