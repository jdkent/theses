@Article{	  kropotov2017,
  abstract	= {{\textcopyright} 2017 Society for Psychophysiological
		  Research. We aimed to determine the functional meaning of
		  latent (hidden) components decomposed from ERPs, in the
		  context of a go/no-go paradigm. To accomplish this, we used
		  a new group blind source separation method, based on joint
		  diagonalization of covariance matrices of ERPs. Four
		  variants of a frequently used go/no-go paradigm were
		  designed, in which operations of reactive cognitive
		  control, such as conflict detection and action inhibition,
		  were independently manipulated. The results showed that a
		  latent component, generated in the anterior cingulate
		  cortex, induced N2/P3 fluctuation only in conditions in
		  which the prepotent model was violated, and thus can be
		  associated with conflict detection operations. In contrast,
		  the two latent components generated in the vicinity of the
		  central sulcus induced P3-like fluctuations in conditions
		  in which the prepared action was suppressed, and thus can
		  be associated with action inhibition operations. The
		  advantages and limitations of the new blind source
		  separation method in relation to ERP research are also
		  discussed.},
  author	= {Kropotov, Juri D. and Ponomarev, Valery A. and Pronina,
		  Marina and J{\"{a}}ncke, Lutz},
  doi		= {10.1111/psyp.12960},
  file		= {:media/Data/Downloads/Kropotov{\_}et{\_}al-2017-Psychophysiology.pdf:pdf},
  issn		= {14698986},
  journal	= {Psychophysiology},
  keywords	= {Blind source separation,Cued go/no-go
		  paradigm,ERPs,Human,Latent components,Reactive cognitive
		  control,cognitive control,eeg,nogo,reactive,task},
  mendeley-tags	= {cognitive control,eeg,nogo,reactive,task},
  number	= {May},
  title		= {{Functional indexes of reactive cognitive control: ERPs in
		  cued go/no-go tasks}},
  year		= {2017}
}

