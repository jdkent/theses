@Article{	  scott2012,
  abstract	= {Advances in technology now make it possible to monitor the
		  activity of the human brain in action, however crudely. As
		  this emerging science continues to offer correlations
		  between neural activity and mental functions, mind and
		  brain may eventually prove to be one. If so, such a full
		  comprehension of the electrochemical bases of mind may
		  render current concepts of ethics, law, and even free will
		  irrelevant.},
  author	= {Scott, Thomas R.},
  doi		= {10.1007/s11948-012-9351-1},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
