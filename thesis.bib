@Article{	  anderson2014,
  abstract	= {Behavioral evidence suggests that the attention-based
		  ability to regulate distraction varies across the day in
		  synchrony with a circadian arousal rhythm that changes
		  across the life span. Using functional magnetic resonance
		  imaging (fMRI), we assessed whether neural activity in an
		  attention control network also varies across the day and
		  with behavioral markers. We tested older adults in the
		  morning or afternoon and younger adults tested in the
		  afternoon using a 1-back task with superimposed
		  distractors, followed by an implicit test for the
		  distractors. Behavioral results replicated earlier findings
		  with older adults tested in the morning better able to
		  ignore distraction than those tested in the afternoon.
		  Imaging results showed that time of testing modulates
		  task-related fMRI signals in older adults and that age
		  differences were reduced when older adults are tested at
		  peak times of day. In particular, older adults tested in
		  the morning activated similar cognitive control regions to
		  those activated by young adults (rostral prefrontal and
		  superior parietal cortex), whereas older adults tested in
		  the afternoon were reliably different; furthermore, the
		  degree to which participants were able to activate the
		  control regions listed above correlated with the ability to
		  suppress distracting information. (PsycINFO Database Record
		  (c) 2014 APA, all rights reserved).},
  author	= {Anderson, John A. E. and Campbell, Karen L. and Amer,
		  Tarek and Grady, Cheryl L. and Hasher, Lynn},
  doi		= {10.1037/a0037243},
  file		= {:media/Data/Downloads/2014-27996-001.pdf:pdf},
  isbn		= {0882-7974, 0882-7974},
  issn		= {1939-1498},
  journal	= {Psychology and Aging},
  keywords	= {aging,chronotype,cognitive control,control
		  network,distraction,fmri,implicit memory,method},
  mendeley-tags	= {aging,cognitive control,fmri,method},
  number	= {3},
  pages		= {648--657},
  pmid		= {24999661},
  title		= {{Timing is everything: Age differences in the cognitive
		  control network are modulated by time of day.}},
  url		= {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0037243},
