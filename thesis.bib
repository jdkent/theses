@Article{	  fan2014,
  abstract	= {Our ability to efficiently process information and
		  generate appropriate responses depends on the processes
		  collectively called cognitive control. Despite a
		  considerable focus in the literature on the cognitive
		  control of information processing, neural mechanisms
		  underlying control are still unclear, and have not been
		  characterized by considering the quantity of information to
		  be processed. A novel and comprehensive account of
		  cognitive control is proposed using concepts from
		  information theory, which is concerned with communication
		  system analysis and the quantification of information. This
		  account treats the brain as an information-processing
		  entity where cognitive control and its underlying brain
		  networks play a pivotal role in dealing with conditions of
		  uncertainty. This hypothesis and theory article justifies
		  the validity and properties of such an account and relates
		  experimental findings to the frontoparietal network under
		  the framework of information theory.},
  annote	= {neural correlates of cognitive control: ACC DLPFC
		  
		  frontoparietal networks processes uncertainty: conflict is
		  a special type of uncertainty.
		  
		  flanker congruent 00000 incongruent 00100 If subjects
		  filter out the irrelevant flanker digit, the predicted
		  uncertainty for the congruent and incongruent conditions is
		  the same 1 bit, with no difference between conditions.
		  however, if subjects cannot filter out the flanker, each
		  stimulus in the incongruent condition indeicates an
		  additional possble response, and increases the uncertainty
		  to 2 bits.
		  
		  average uncertainty is less than 1 bit, with RT difference
		  of 50-150 ms},
  author	= {Fan, Jin},
  doi		= {10.3389/fnhum.2014.00680},
  file		= {:media/Data/Downloads/fnhum-08-00680.pdf:pdf},
  isbn		= {1662-5161 (Electronic)$\backslash$r1662-5161 (Linking)},
  issn		= {1662-5161},
  journal	= {Frontiers in human neuroscience},
  keywords	= {cognitive control,entropy,frontoparietal
		  network,information theory,review,theory,uncertainty},
  mendeley-tags	= {cognitive control,review,theory},
  number	= {September},
  pages		= {680},
  pmid		= {25228875},
  title		= {{An information theory account of cognitive control.}},
  url		= {http://journal.frontiersin.org/article/10.3389/fnhum.2014.00680/abstract{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/25228875{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4151034},
  volume	= {8},
  year		= {2014}
}

