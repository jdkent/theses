@Article{	  boot2013,
  abstract	= {To draw causal conclusions about the efficacy of a
		  psychological intervention, researchers must compare the
		  treatment condition with a control group that accounts for
		  improvements caused by factors other than the treatment.
		  Using an active control helps to control for the
		  possibility that improvement by the experimental group
		  resulted from a placebo effect. Although active control
		  groups are superior to “no-contact” controls, only when
		  the active control group has the same expectation of
		  improvement as the experimental group can we attribute
		  differential improvements to the potency of the treatment.
		  Despite the need to match expectations between treatment
		  and control groups, almost no psychological interventions
		  do so. This failure to control for expectations is not a
		  minor omission—it is a fundamental design flaw that
		  potentially undermines any causal inference. We illustrate
		  these principles with a detailed example from the
		  video-game-training literature showing how the use of an
		  active control group does not eliminate expectation
		  differences. The problem permeates other interventions as
		  well, including those targeting mental health, cognition,
		  and educational achievement. Fortunately, measuring
		  expectations and adopting alternative experimental designs
		  makes it possible to control for placebo effects, thereby
		  increasing confidence in the causal efficacy of
		  psychological interventions.},
  author	= {Boot, Walter R and Simons, Daniel J and Stothart, Cary and
		  Stutts, Cassie},
  doi		= {10.1177/1745691613491271},
  file		= {:home/james/.local/share/data/Mendeley Ltd./Mendeley
		  Desktop/Downloaded/Boot et al. - 2013 - The Pervasive
		  Problem With Placebos in Psychology Why Active Control
		  Groups Are Not Sufficient to Rule Out Placebo.pdf:pdf},
  isbn		= {1745-6916},
  issn		= {1745-6916},
  journal	= {Perspectives on Psychological Science},
  keywords	= {a psy-,about the efficacy of,chological
		  intervention,cognitive training,demand
		  characteristics,intervention design,method,placebo
		  effect,research methods,researchers must compare the,to
		  draw causal conclusions},
  mendeley-tags	= {cognitive training,method},
  number	= {4},
  pages		= {445--454},
  pmid		= {26173122},
  title		= {{The Pervasive Problem With Placebos in Psychology: Why
		  Active Control Groups Are Not Sufficient to Rule Out
		  Placebo Effects}},
  url		= {http://pps.sagepub.com/content/8/4/445.abstract},
  volume	= {8},
  year		= {2013}
}

