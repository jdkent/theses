@Article{	  cary2017,
  abstract	= {Current research in connectomics highlights that
		  self-organized functional networks or "communities" of
		  cortical areas can be detected in the adult brain. This
		  perspective may provide clues to mechanisms of treatment
		  response in psychiatric conditions. Here we examine
		  functional brain community topology based on resting-state
		  fMRI in adult Attention-Deficit/Hyperactivity Disorder
		  (ADHD; n = 22) and controls (n = 31). We sought to
		  evaluate ADHD patterns in adulthood and their modification
		  by short term stimulants administration. Participants with
		  ADHD were scanned one or two weeks apart, once with
		  medication and once without; comparison participants were
		  scanned at one time-point. Functional connectivity was
		  estimated from these scans and community detection applied
		  to determine cortical network topology. Measures of change
		  in connectivity profile were calculated via a graph
		  measure, termed the Node Dissociation Index (NDI). Compared
		  to controls, several cortical networks had atypical
		  connectivity in adults with ADHD when withholding
		  stimulants, as measured by NDI. In most networks stimulants
		  significantly reduced, but did not eliminate, differences
		  in the distribution of connections between key brain
		  systems relative to the control sample. These findings
		  provide an enriched model of connectivity in ADHD and
		  demonstrate how stimulants may exert functional effects by
		  altering connectivity profiles in the brain.},
  annote	= {aim1: get a picture of the community structure of people
		  with ADHD
		  
		  aim2: see how medication changes the community structure
		  
		  Question: would like to see CDI as a function of data
		  remaining (volumes not scrubbed)},
  author	= {Cary, Robert P. and Ray, Siddharth and Grayson, David S.
		  and Painter, Julia and Carpenter, Samuel and Maron, Leeza
		  and Sporns, Olaf and Stevens, Alexander A. and Nigg, Joel
		  T. and Fair, Damien A.},
  doi		= {10.1093/cercor/bhw209},
  file		= {:media/Data/Downloads/Cary et al cerebral cortex
		  2016.pdf:pdf},
  isbn		= {1047-3211, 1047-3211},
  issn		= {14602199},
  journal	= {Cerebral Cortex},
  keywords	= {ADHD,Functional connectivity,MRI,Resting
		  state,Stimulants},
  number	= {8},
  pages		= {3970--3979},
  pmid		= {27422412},
  title		= {{Network structure among brain systems in adult ADHD is
		  uniquely modified by stimulant administration}},
  volume	= {27},
  year		= {2017}
}

